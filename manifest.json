 {
    "name": "OIT Pending Timesheet Notifier",
    "version": "1.0",
    "description": "An in house timesheet notifier for OIT employees",
    "permissions": ["notifications","declarativeContent","identity","storage"],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/get_started16.png",
        "32": "images/get_started32.png",
        "48": "images/get_started48.png",
        "128": "images/get_started128.png"
      }
    },
    "icons": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    },
    "oauth2": {
      "client_id": "833796990833-27guvan3n0lntdgmjf73mvsksgtoite7.apps.googleusercontent.com",
      "scopes": ["https://www.googleapis.com/auth/gmail.readonly"]
    },
    "manifest_version": 2
  }